{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-resizable-pane": {
      "projectType": "library",
      "root": "projects/angular-resizable-pane",
      "sourceRoot": "projects/angular-resizable-pane/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/angular-resizable-pane/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-resizable-pane/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/angular-resizable-pane/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/angular-resizable-pane/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "5f633c96-e9be-4b62-ba5d-5ba464f02bee"
  }
}
